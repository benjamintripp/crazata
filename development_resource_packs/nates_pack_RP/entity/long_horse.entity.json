{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "nate:long_horse",
			"materials": {
				"default": "horse_leather_armor"
			},
			"textures": {
				"base_brown": "textures/entity/long_horse/base_brown",
				"base_white": "textures/entity/long_horse/base_white",
				"base_creamy": "textures/entity/long_horse/base_creamy",
				"base_chestnut": "textures/entity/long_horse/base_chestnut",
				"base_black": "textures/entity/long_horse/base_black",
				"base_gray": "textures/entity/long_horse/base_gray",
				"base_darkbrown": "textures/entity/long_horse/base_darkbrown",
				"markings_none": "textures/entity/long_horse/markings_none",
				"markings_white": "textures/entity/long_horse/markings_white",
				"markings_whitefield": "textures/entity/long_horse/markings_whitefield",
				"markings_whitedots": "textures/entity/long_horse/markings_whitedots",
				"markings_blackdots": "textures/entity/long_horse/markings_blackdots",
				"armor_none": "textures/entity/long_horse/armor_none",
				"armor_leather": "textures/entity/long_horse/armor_leather",
				"armor_iron": "textures/entity/long_horse/armor_iron",
				"armor_gold": "textures/entity/long_horse/armor_gold",
				"armor_diamond": "textures/entity/long_horse/armor_diamond"
			},
			"geometry": {
				"default": "geometry.long_horse"
			},
			"animations": {
				"baby_transform": "animation.long_horse.baby_transform",
				"walk": "animation.long_horse.walk",
				"eat": "animation.long_horse.eat",
				"rear": "animation.long_horse.rear",
				"tail": "animation.long_horse.tail",
				"look_at_player": "animation.long_horse.look_at_player"
			},
			"scripts": {
				"initialize": ["variable.eat_anim = 0;", "variable.stand_anim = 0;", "variable.leg_stand_factor = 0;", "variable.leg_x_rot_anim = 0;"],
				"pre_animation": [
					"variable.armor_texture_slot = query.armor_texture_slot(1);",
					"variable.eat_anim = math.clamp(variable.eat_anim + (query.is_grazing ? ((1.0 - variable.eat_anim) * 0.4 + 0.05) : -variable.eat_anim * 0.4 - 0.05) * query.delta_time * 20.0, 0.0, 1.0);",
					"variable.head_x_rot = query.target_x_rotation + (query.modified_move_speed > 0.2 ? (math.cos(query.modified_distance_moved * 11.46) * 11.46 * query.modified_move_speed + query.modified_move_speed * 11.46) : 0.0);",
					"variable.inverse_max_stand_eat = (1.0 - math.max(variable.stand_anim, variable.eat_anim));",
					"variable.leg_stand_factor = math.cos((query.modified_distance_moved * 38.38) + 180.0);",
					"variable.leg_x_rot_anim = variable.leg_stand_factor * 45.8 * query.modified_move_speed;"
				],
				"animate": [
					{
						"baby_transform": "query.is_baby"
					},
					"walk",
					"look_at_player",
					{
						"rear": "variable.stand_anim > 0.0"
					},
					{
						"eat": "!query.is_standing"
					},
					{
						"tail": "Math.mod(q.life_time, 10) < 0.5"
					}
				]
			},
			"render_controllers": ["controller.render.horse.v3"],
			"spawn_egg": {
				"base_color": "#35b8af",
				"overlay_color": "#01de84"
			}
		}
	}
}