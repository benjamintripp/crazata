{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.everything_bear": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.default",
            "Texture.lazy",
            "Texture.worried",
            "Texture.playful",
