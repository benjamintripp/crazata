{
	"format_version": "1.8.0",
	"animations": {
		"animation.spinning_chicken.baby_transform": {
			"loop": true,
			"bones": {
				"head": {
					"scale": 2
				}
			}
		},
		"animation.spinning_chicken.general": {
			"loop": true,
			"bones": {
				"wing0": {
					"rotation": [0, 0, "variable.wing_flap - this"]
				},
				"wing1": {
					"rotation": [0, 0, "-variable.wing_flap - this"]
				},
				"wholechicken": {
					"rotation": [0, "query.anim_time * 200", 0]
				}
			}
		},
		"animation.spinning_chicken.move": {
			"loop": true,
			"anim_time_update": "query.modified_distance_moved",
			"bones": {
				"leg0": {
					"rotation": ["math.cos(query.anim_time * 38.17) *  80.0", 0, 0]
				},
				"leg1": {
					"rotation": ["math.cos(query.anim_time * 38.17) * -80.0", 0, 0]
				},
				"wholechicken": {
					"rotation": [0, "query.modified_distance_moved * 50", 0]
				}
			}
		}
	}
}